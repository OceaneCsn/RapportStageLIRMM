	author = {Fulco, Charles P and Nasser, Joseph and Jones, Thouis R and Munson, Glen and Bergman, Drew T and Subramanian, Vidya and Grossman, Sharon R and Anyoha, Rockwell and Patwardhan, Tejal A and Nguyen, Tung H and Kane, Michael and Doughty, Benjamin and Perez, Elizabeth M and Durand, Neva C and Stamenova, Elena K and Lieberman Aiden, Erez and Lander, Eric S and Engreitz, Jesse M},
	month = jan,
	year = {2019},
	file = {Fulco et al. - 2019 - Activity-by-Contact model of enhancer specificity .pdf:/home/oceane/Zotero/storage/WQZMEZDP/Fulco et al. - 2019 - Activity-by-Contact model of enhancer specificity .pdf:application/pdf}
}

