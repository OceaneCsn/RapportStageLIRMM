	month = nov,
	year = {2013},
	pmid = {24141950},
	pmcid = {PMC3838900},
	pages = {290--294},
	file = {PubMed Central Full Text PDF:/home/oceane/Zotero/storage/LG68GD7L/Jin et al. - 2013 - A high-resolution map of three-dimensional chromat.pdf:application/pdf}
}

