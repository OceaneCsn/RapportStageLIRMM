	pmid = {30262496},
	pages = {1341--1345},
	file = {Full Text PDF:/home/oceane/Zotero/storage/6R2XRGGJ/Furlong et Levine - 2018 - Developmental enhancers and chromosome topology.pdf:application/pdf;Snapshot:/home/oceane/Zotero/storage/PHERECFP/tab-figures-data.html:text/html}
}

